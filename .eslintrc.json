{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "prettier"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "prettier/prettier": "error",
    "react/prop-types": "off"// позже нужно бы поразмышлять об том нафига я это отключил, может есть другой вариант избавиться от ошибки...
  }
}
